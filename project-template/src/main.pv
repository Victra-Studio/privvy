class Router {
    constructor() {
        this.routes = []
    }
    
    fun addRoute(path, handler) {
        print("Registered route: " + path)
        // In a real web app, this would register the route
    }
    
    fun navigate(path) {
        print("Navigating to: " + path)
        // In a real web app, this would call the handler
    }
}

class App {
    constructor(name) {
        this.name = name
        this.router = new Router()
    }
    
    fun setup() {
        print("Setting up " + this.name + "...")
        
        // Register routes
        this.router.addRoute("/", "home")
        this.router.addRoute("/about", "about")
        this.router.addRoute("/contact", "contact")
    }
    
    fun run() {
        print("")
        print("Starting " + this.name + "...")
        this.router.navigate("/")
        print("")
        print(this.name + " is running!")
    }
}

// Create and run the application
